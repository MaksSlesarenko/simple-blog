<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Admin{% endblock %}</title>
        {% block stylesheets %}
            <link href="{{ asset('bundles/app/css/bootstrap.css') }}" rel="stylesheet" />
            <link href="{{ asset('bundles/app/css/management.css') }}" rel="stylesheet" />
            <link href='https://fonts.googleapis.com/css?family=Roboto:100,300,500' rel='stylesheet' type='text/css'>
        {% endblock %}
        {% block javascripts %}
            <script data-main="{{ asset('bundles/app/js/config-admin.js') }}"
                    src="{{ asset('bundles/app/js/vendor/require.js') }}"></script>
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        {% block body %}{% endblock %}
    </body>
</html>
